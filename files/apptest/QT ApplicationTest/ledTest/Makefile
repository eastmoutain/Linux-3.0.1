unix_mkipk:
	@ if [ -e /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/data ]; then echo "ERROR: /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/data exists!"; exit 1; fi; if [ -e /root/AppTest/ledTest/ipkg/qpe-ledtest__arm.ipk ]; then echo "Package qpe-ledtest__arm.ipk already exists!"; exit 0; fi; IPK_GROUP_PROJECTS="/root/AppTest/ledTest"; for i in $${IPK_GROUP_PROJECTS};do make -C $$i -f Makefile unix_mkipk_deps INSTALL_ROOT=/root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/data/opt/Qtopia || exit $$?; done; make_ipk_file() { cd /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/data; if [ ! -z "$$1" ]; then lang="$$1"; suffix="_i18n_$$lang"; deps="qpe-ledtest"; if [ "`echo qpe-ledtest | cut -c -4`" = "qpe-" -a "qpe-ledtest" != "qpe-taskbar" ]; then deps="$$deps qpe-taskbar_i18n_$$lang"; fi; path="opt/Qtopia/i18n/$$lang"; if [ ! -d "/root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/data/$$path" ]; then return; fi; else suffix=; deps=`cat /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/DEPS`; deps=`echo $$deps | sort -u`; path="opt/Qtopia"; fi; echo "Package: qpe-ledtest$$suffix" > /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; echo "Maintainer: Unspecified" >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; echo "License: GPL" >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; echo "Version: " >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; echo "Arch: arm" >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; echo "Architecture: arm" >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; echo "Description: No description written for this package" >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; echo -n "Depends: " >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; echo $$deps >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; echo -n "Files:" >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; find $$path -type f -o -type l -printf "/%P " >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; if [ ! -x "`which mkfs.jffs2`" ]; then echo -e "\nInstalled-Size: $$(du -h -s $$path | awk '{print $$1}')" >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; else echo -e "\nInstalled-Size: $$(mkfs.jffs2 -r $$path | wc -c)" >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; fi; echo >> /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control/control; for i in preinst prerm postinst postrm;do export FILE_LOC=/root/AppTest/ledTest/qpe-ledtest.$$i; if [ -e $$FILE_LOC ]; then install -c $$FILE_LOC /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/$$i; fi; done; cd /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest; echo "2.0" > debian-binary; cd /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control; tar --remove-files -cz --owner=0 --group=0 -f /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/control.tar.gz ./control; cd /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/data; tar --remove-files -cz --owner=0 --group=0 -f /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/data.tar.gz $$path; if [ ! -e /root/AppTest/ledTest/ipkg ];then mkdir -p /root/AppTest/ledTest/ipkg;fi; cd /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest; tar --remove-files -cz --owner=0 --group=0 -f /root/AppTest/ledTest/ipkg/qpe-ledtest__arm$$suffix.ipk ./debian-binary ./control.tar.gz ./data.tar.gz `for i in preinst prerm postinst postrm;do if [ -e $$i ]; then echo $$i; fi; done`; }; LANGUAGES="en_US"; for lang in $${LANGUAGES}; do make_ipk_file $$lang; done; [ -d /root/AppTest/ledTest/.packaging-working-dir/ipk/qpe-ledtest/data/op