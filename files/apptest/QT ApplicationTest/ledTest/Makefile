.obj/release-shared/ledtest.o: ledtest.cpp ledtest.h \
		.ui/release-shared/UI_ledTest.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/ledtest.o ledtest.cpp

.ui/release-shared/UI_ledTest.h: UI_ledTest.ui 
	$(UIC) UI_ledTest.ui -o .ui/release-shared/UI_ledTest.h

.ui/release-shared/UI_ledTest.cpp: .ui/release-shared/UI_ledTest.h UI_ledTest.ui 
	$(UIC) UI_ledTest.ui -i UI_ledTest.h -o .ui/release-shared/UI_ledTest.cpp

.obj/release-shared/UI_ledTest.o: .ui/release-shared/UI_ledTest.cpp .ui/release-shared/UI_ledTest.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/UI_ledTest.o .ui/release-shared/UI_ledTest.cpp

