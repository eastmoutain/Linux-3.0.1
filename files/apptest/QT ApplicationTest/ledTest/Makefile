.obj/release-shared/moc_UI_ledTest.o: .moc/release-shared/moc_UI_ledTest.cpp .ui/release-shared/UI_ledTest.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_UI_ledTest.o .moc/release-shared/moc_UI_ledTest.cpp

