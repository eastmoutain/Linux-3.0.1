unix_mkipk:
	@ if [ -e /root/serial/.packaging-working-dir/ipk/qpe-serialport/data ]; then echo "ERROR: /root/serial/.packaging-working-dir/ipk/qpe-serialport/data exists!"; exit 1; fi; if [ -e /root/serial/ipkg/qpe-serialport__arm.ipk ]; then echo "Package qpe-serialport__arm.ipk already exists!"; exit 0; fi; IPK_GROUP_PROJECTS="/root/serial"; for i in $${IPK_GROUP_PROJECTS};do make -C $$i -f Makefile unix_mkipk_deps INSTALL_ROOT=/root/serial/.packaging-working-dir/ipk/qpe-serialport/data/opt/Qtopia || exit $$?; done; make_ipk_file() { cd /root/serial/.packaging-working-dir/ipk/qpe-serialport/data; if [ ! -z "$$1" ]; then lang="$$1"; suffix="_i18n_$$lang"; deps="qpe-serialport"; if [ "`echo qpe-serialport | cut -c -4`" = "qpe-" -a "qpe-serialport" != "qpe-taskbar" ]; then deps="$$deps qpe-taskbar_i18n_$$lang"; fi; path="opt/Qtopia/i18n/$$lang"; if [ ! -d "/root/serial/.packaging-working-dir/ipk/qpe-serialport/data/$$path" ]; then return; fi; else suffix=; deps=`cat /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/DEPS`; deps=`echo $$deps | sort -u`; path="opt/Qtopia"; fi; echo "Package: qpe-serialport$$suffix" > /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; echo "Maintainer: Unspecified" >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; echo "License: GPL" >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; echo "Version: " >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; echo "Arch: arm" >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; echo "Architecture: arm" >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; echo "Description: No description written for this package" >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; echo -n "Depends: " >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; echo $$deps >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; echo -n "Files:" >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; find $$path -type f -o -type l -printf "/%P " >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; if [ ! -x "`which mkfs.jffs2`" ]; then echo -e "\nInstalled-Size: $$(du -h -s $$path | awk '{print $$1}')" >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; else echo -e "\nInstalled-Size: $$(mkfs.jffs2 -r $$path | wc -c)" >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; fi; echo >> /root/serial/.packaging-working-dir/ipk/qpe-serialport/control/control; for i in preinst prerm postinst postrm;do export FILE_LOC=/root/serial/qpe-serialport.$$i; if [ -e $$FILE_LOC ]; then install -c $$FILE_LOC /root/serial/.packaging-working-dir/ipk/qpe-serialport/$$i; fi; done; cd /root/serial/.packaging-working-dir/ipk/qpe-serialport; echo "2.0" > debian-binary; cd /root/serial/.packaging-working-dir/ipk/qpe-serialport/control; tar --remove-files -cz --owner=0 --group=0 -f /root/serial/.packaging-working-dir/ipk/qpe-serialport/control.tar.gz ./control; cd /root/serial/.packaging-working-dir/ipk/qpe-serialport/data; tar --remove-files -cz --owner=0 --group=0 -f /root/serial/.packaging-working-dir/ipk/qpe-serialport/data.tar.gz $$path; if [ ! -e /root/serial/ipkg ];then mkdir -p /root/serial/ipkg;fi; cd /root/serial/.packaging-working-dir/ipk/qpe-serialport; tar --remove-files -cz --owner=0 --group=0 -f /root/serial/ipkg/qpe-serialport__arm$$suffix.ipk ./debian-binary ./control.tar.gz ./data.tar.gz `for i in preinst prerm postinst postrm;do if [ -e $$i ]; then echo $$i; fi; done`; }; LANGUAGES="en_US"; for lang in $${LANGUAGES}; do make_ipk_file $$lang; done; [ -d /root/serial/.packaging-working-dir/ipk/qpe-serialport/data/opt/Qtopia/i18n ] && rm -r /root/serial/.packaging-working-dir/ipk/qpe-serialport/data/opt/Qtopia/i18n; make_ipk_file; rm -rf /root/serial/.packaging-working-dir/ipk/qpe-serialport

