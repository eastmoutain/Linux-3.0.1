.ui/release-shared/main_form_base.h: main_form_base.ui 
	$(UIC) main_form_base.ui -o .ui/release-shared/main_form_base.h

.ui/release-shared/main_form_base.cpp: .ui/release-shared/main_form_base.h main_form_base.ui 
	$(UIC) main_form_base.ui -i main_form_base.h -o .ui/release-shared/main_form_base.cpp

.obj/release-shared/main_form_base.o: .ui/release-shared/main_form_base.cpp .ui/release-shared/main_form_base.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/main_form_base.o .ui/release-shared/main_form_base.cpp

