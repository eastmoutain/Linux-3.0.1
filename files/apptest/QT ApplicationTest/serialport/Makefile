lupdate:
	@ cd /root/serial; TRANSFILES=; TRANSLATABLES="$(TRANSLATABLES)"; for transfile in $$TRANSLATABLES; do [ -f $$transfile ] && TRANSFILES="$$TRANSFILES $$transfile"; done; TRANSLATIONS=""; for lang in $$TRANSLATIONS; do /root/yizhi/qtopia-free-2.2.0/dqt/bin/lupdate $$TRANSFILES -ts serialport-$$lang.ts; done

